Use cWebButton.pkg

Class cWebVoice is a cWebButton
    
    Procedure Construct_Object
        Forward Send Construct_Object
        
        Set psJSClass to "dfcc.WebVoice"

    End_Procedure
    
    { MethodType = Event }
    Procedure OnResponse String sTranscript
        
    End_Procedure

    Procedure End_Construct_Object
        Forward Send End_Construct_Object
        
        WebPublishProcedure OnResponse
    End_Procedure

End_Class
